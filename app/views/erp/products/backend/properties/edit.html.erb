<% content_for :title do %>Cập Nhật Thuộc Tính: <%= @property.get_name %><% end %>
<% content_for :page_title do %><a href="<%= erp_products.backend_properties_path %>"><i class="fa fa-gg"></i> Danh Sách Thuộc Tính</a> / Cập Nhật Thuộc Tính: <%= @property.get_name %><% end %>
<% content_for :page_toolbar do %><%= erp_component('button/create', {text: 'Tạo thuộc tính mới', href: erp_products.new_backend_property_path, class: 'btn-sm' }) %><% end %>
<div class="row"><div class="col-md-6"><% if @property.errors.any? %><%= erp_form_control('form_error_message') %><% end %><%= render 'form', property: @property %></div></div>